
  CREATE TABLE "C##SAM_05"."CHK_GOOD" 
   (	"SEQ" NUMBER NOT NULL ENABLE, 
	"BOARD_TYPE" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
	"P_SEQ" NUMBER NOT NULL ENABLE, 
	"M_ID" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
	"USE_YN" VARCHAR2(1 BYTE) DEFAULT 'Y' NOT NULL ENABLE, 
	"REG_ID" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
	"REG_DATE" VARCHAR2(14 BYTE) DEFAULT TO_CHAR(SYSDATE,'YYYYMMDDHH24MISS') NOT NULL ENABLE, 
	"MOD_ID" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
	"MOD_DATE" VARCHAR2(14 BYTE) DEFAULT TO_CHAR(SYSDATE,'YYYYMMDDHH24MISS') NOT NULL ENABLE, 
	"NOTE" NUMBER(24,0) NOT NULL ENABLE
   ) SEGMENT CREATION IMMEDIATE 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 
 NOCOMPRESS LOGGING
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "USERS" ;




  CREATE TABLE "C##SAM_05"."PDS_BOARD" 
   (	"SEQ" NUMBER DEFAULT 0 NOT NULL ENABLE, 
	"P_SEQ" NUMBER DEFAULT 0, 
	"G_SEQ" NUMBER DEFAULT 0, 
	"STEP" NUMBER DEFAULT 0, 
	"TITLE" VARCHAR2(200 BYTE), 
	"TITLE_EN" VARCHAR2(200 BYTE), 
	"CONTENTS" VARCHAR2(4000 BYTE), 
	"INFO" VARCHAR2(4000 BYTE), 
	"DEPT_NAME" VARCHAR2(100 BYTE), 
	"DEPT_MEM" VARCHAR2(100 BYTE), 
	"DEL_YN" VARCHAR2(1 BYTE) DEFAULT 'N', 
	"NOTE" VARCHAR2(1000 BYTE), 
	"REG_ID" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
	"REG_DATE" VARCHAR2(20 BYTE) DEFAULT TO_CHAR(SYSDATE,'YYYYMMDD HH24:MI:SS') NOT NULL ENABLE, 
	"MOD_ID" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
	"MOD_DATE" VARCHAR2(20 BYTE) DEFAULT TO_CHAR(SYSDATE,'YYYYMMDD HH24:MI:SS') NOT NULL ENABLE, 
	"VIEWS" NUMBER DEFAULT 0, 
	"BOARD_TYPE" VARCHAR2(20 BYTE) DEFAULT 0 NOT NULL ENABLE, 
	"AREA" VARCHAR2(20 BYTE) DEFAULT 0, 
	"FIELD_TYPE" VARCHAR2(20 BYTE) DEFAULT 0, 
	"LINK_KO" VARCHAR2(200 BYTE), 
	"LINK_EN" VARCHAR2(200 BYTE), 
	"COMMENT_SEQ" NUMBER DEFAULT -1, 
	"ORIGIN_FILE_NAME1" VARCHAR2(100 BYTE), 
	"TARGET_FILE_NAME1" VARCHAR2(100 BYTE), 
	"FILE_EXT1" VARCHAR2(4 BYTE), 
	"FILE_SIZE1" VARCHAR2(12 BYTE) DEFAULT 0, 
	"FILE_PATH" VARCHAR2(200 BYTE), 
	"ORIGIN_FILE_NAME2" VARCHAR2(100 BYTE), 
	"ORIGIN_FILE_NAME3" VARCHAR2(100 BYTE), 
	"ORIGIN_FILE_NAME4" VARCHAR2(100 BYTE), 
	"TARGET_FILE_NAME2" VARCHAR2(100 BYTE), 
	"TARGET_FILE_NAME3" VARCHAR2(100 BYTE), 
	"TARGET_FILE_NAME4" VARCHAR2(100 BYTE), 
	"FILE_EXT2" VARCHAR2(4 BYTE), 
	"FILE_EXT3" VARCHAR2(4 BYTE), 
	"FILE_EXT4" VARCHAR2(4 BYTE), 
	"FILE_SIZE2" VARCHAR2(12 BYTE) DEFAULT 0, 
	"FILE_SIZE3" VARCHAR2(12 BYTE) DEFAULT 0, 
	"FILE_SIZE4" VARCHAR2(12 BYTE) DEFAULT 0, 
	"M_ID" VARCHAR2(20 BYTE), 
	"ANS_CNT" NUMBER, 
	"G_SEQ_ORDER" NUMBER DEFAULT 0, 
	"GOOD" NUMBER DEFAULT 0, 
	"CATEGORY" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
	"SORT" NUMBER DEFAULT 1, 
	 CONSTRAINT "PDS_BOARD_PK" PRIMARY KEY ("SEQ")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS 
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "USERS"  ENABLE
   ) SEGMENT CREATION IMMEDIATE 
